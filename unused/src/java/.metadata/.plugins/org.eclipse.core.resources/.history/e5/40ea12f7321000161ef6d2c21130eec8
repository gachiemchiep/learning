
public class Account {
	private String name;
	private int balance;
	
	public Account() {
		
	}
	
	public Account(String name_, int balance_) {
		this.name = name_;
		this.balance = balance_;
	}
	
	public void display() {
		System.out.println("Account name:" + name);
		System.out.println("Balance:" + balance);
	}
}
