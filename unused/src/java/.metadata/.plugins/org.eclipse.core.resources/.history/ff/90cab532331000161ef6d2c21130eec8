package abstract_class;

class SavingsAccount extends Account {
	
	private int time;
	
	public SavingsAccount(String name_, int balance_, int time_) {
		super(name_, balance_);
		this.time = time_;
	}
	
	public void setTime(int time_) {
		this.time = time_;
	}
	
	public int getTime() {
		return this.time;
	}
	
	public void display() {
		System.out.println("Account name:" + this.getName());
		System.out.println("Balance:" + this.getBalance());
		System.out.println("Time:" + this.getTime());
		
	}
	
}
